{
  "name": "modern-portfolio",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "build:ci": "NODE_OPTIONS='--max-old-space-size=8192' next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:ci": "next lint --quiet --format=compact",
    "type-check": "tsc --noEmit --incremental --tsBuildInfoFile .next/cache/tsc-check.tsbuildinfo",
    "type-check:ci": "tsc --noEmit --incremental false",
    "generate-sitemap": "node scripts/generate-sitemap.js",
    "postbuild": "npm run generate-sitemap",
    "analyze": "ANALYZE=true npm run build",
    "clean": "rm -rf .next out node_modules/.cache",
    "clean:cache": "rm -rf .next/cache",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "build:analyze": "cross-env ANALYZE=true npm run build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:install": "playwright install",
    "e2e": "npm run test:e2e",
    "e2e:ui": "npm run test:e2e:ui",
    "e2e:headed": "npm run test:e2e:headed",
    "e2e:debug": "npm run test:e2e:debug",
    "e2e:report": "npm run test:e2e:report",
    "test:install": "npm run test:e2e:install",
    "test:all": "npm run test:run && npm run test:e2e",
    "test:ci": "npm run test:coverage && npm run test:e2e",
    "validate": "npm run type-check && npm run lint",
    "prebuild": "npm run validate",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:reset": "prisma migrate reset",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "postinstall": "prisma generate"
  },
