{
  "name": "modern-portfolio",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "bun@1.1.0",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "build:ci": "next build",
    "start": "next start",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "lint:ci": "eslint src --ext .ts,.tsx,.js,.jsx --quiet",
    "type-check": "tsc --noEmit --incremental --tsBuildInfoFile .next/cache/tsc-check.tsbuildinfo",
    "type-check:ci": "tsc --noEmit --incremental --tsBuildInfoFile .next/cache/tsc-check.tsbuildinfo",
    "generate-sitemap": "bun run scripts/generate-sitemap.js",
    "postbuild": "bun run generate-sitemap",
    "prepare": "husky",
    "analyze": "ANALYZE=true bun run build",
    "clean": "rm -rf .next out node_modules/.cache",
    "clean:cache": "rm -rf .next/cache",
    "dev:debug": "bun --bun next dev --inspect",
    "build:analyze": "ANALYZE=true bun run build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run --no-file-parallelism --passWithNoTests",
    "test:coverage": "vitest run --no-file-parallelism --coverage --passWithNoTests",
    "test:watch": "vitest --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:install": "playwright install",
    "e2e": "bun run test:e2e",
    "e2e:ui": "bun run test:e2e:ui",
    "e2e:headed": "bun run test:e2e:headed",
    "e2e:debug": "bun run test:e2e:debug",
    "e2e:report": "bun run test:e2e:report",
    "test:install": "bun run test:e2e:install",
    "test:all": "bun run test:run && bun run test:e2e",
    "test:ci": "bun run test:coverage && bun run test:e2e",
    "ci:local": "concurrently -n lint,type,test \"bun run lint:ci\" \"bun run type-check:ci\" \"bun run test:run\"",
    "ci:quick": "bun run type-check:ci",
    "ci:full": "bun run ci:quick && bun run build:ci",
    "ci:changed": "bunx lint-staged && bun run type-check:ci",
    "verify": "lefthook run ci",
    "pre-push": "bun run ci:quick",
    "validate": "bun run type-check && bun run lint",
    "prebuild": "bun run type-check",
    "db:generate": "bunx prisma generate",
    "db:push": "bunx prisma db push",
    "db:migrate": "bunx prisma migrate dev",
    "db:migrate:deploy": "bunx prisma migrate deploy",
    "db:migrate:reset": "bunx prisma migrate reset",
    "db:seed": "bun run prisma/seed.ts",
    "db:studio": "bunx prisma studio",
    "postinstall": "bunx prisma generate"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@prisma/client": "^6.13.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "2.1.8",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toggle": "^1.1.9",
    "@tanstack/react-form": "^1.25.0",
    "@tanstack/react-query": "^5.84.1",
    "@tanstack/react-query-devtools": "^5.84.1",
    "@tanstack/react-table": "^8.21.3",
    "@types/dompurify": "^3.0.5",
    "@vercel/analytics": "^1.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "critters": "^0.0.23",
    "date-fns": "^4.1.0",
    "dompurify": "^3.3.0",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.536.0",
    "next": "^15.5.7",
    "next-themes": "^0.4.6",
    "postcss": "^8.5.6",
    "react": "^19.1.1",
    "react-day-picker": "^9.8.1",
    "react-dom": "^19.1.1",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.62.0",
    "react-icons": "^5.5.0",
    "react-intersection-observer": "^9.16.0",
    "react-resizable-panels": "^3.0.4",
    "react-simple-typewriter": "^5.0.1",
    "recharts": "^2.15.4",
    "resend": "^4.8.0",
    "sharp": "^0.34.3",
    "sonner": "^2.0.7",
    "swiper": "^11.2.10",
    "vaul": "^1.1.2",
    "web-vitals": "^5.1.0",
    "zod": "^4.0.15"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.32.0",
    "@next/bundle-analyzer": "^15.4.5",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bun": "1.3.4",
    "@types/node": "^24.2.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^3.2.4",
    "concurrently": "^9.2.0",
    "eslint": "^9.32.0",
    "eslint-config-next": "^15.4.5",
    "eslint-formatter-compact": "^8.40.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.2.7",
    "playwright": "^1.56.1",
    "prisma": "^6.13.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "tsx": "^4.20.3",
    "tw-animate-css": "^1.3.6",
    "typescript": "5.8.3",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "eslint --fix"
    ]
  }
}
