{
  "name": "modern-portfolio",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "bun@1.3.5",
  "engines": {
    "node": ">=22.0.0 <25.0.0"
  },
  "scripts": {
    "dev": "rm -f .next/dev/lock && next dev",
    "build": "next build",
    "build:ci": "next build",
    "start": "next start",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "lint:ci": "eslint src --ext .ts,.tsx,.js,.jsx --quiet",
    "type-check": "tsc --noEmit --incremental --tsBuildInfoFile .next/cache/tsc-check.tsbuildinfo",
    "type-check:ci": "tsc --noEmit --incremental --tsBuildInfoFile .next/cache/tsc-check.tsbuildinfo",
    "generate-sitemap": "bun run scripts/generate-sitemap.js",
    "postbuild": "bun run generate-sitemap",
    "prepare": "husky",
    "analyze": "ANALYZE=true bun run build",
    "clean": "rm -rf .next out node_modules/.cache",
    "clean:cache": "rm -rf .next/cache",
    "dev:debug": "bun --bun next dev --inspect",
    "build:analyze": "ANALYZE=true bun run build",
    "test": "vitest",
    "test:unit": "vitest run --passWithNoTests --exclude='**/rate-limiter-load*'",
    "test:ui": "vitest --ui",
    "test:run": "vitest run --no-file-parallelism --passWithNoTests",
    "test:coverage": "vitest run --no-file-parallelism --coverage --passWithNoTests",
    "test:watch": "vitest --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:install": "playwright install",
    "e2e": "bun run test:e2e",
    "e2e:ui": "bun run test:e2e:ui",
    "e2e:headed": "bun run test:e2e:headed",
    "e2e:debug": "bun run test:e2e:debug",
    "e2e:report": "bun run test:e2e:report",
    "test:install": "bun run test:e2e:install",
    "test:all": "bun run test:run && bun run test:e2e",
    "test:ci": "bun run test:coverage && bun run test:e2e",
    "ci:local": "concurrently -n lint,type,test \"bun run lint:ci\" \"bun run type-check:ci\" \"bun run test:run\"",
    "ci:quick": "bun run type-check:ci",
    "ci:full": "bun run ci:quick && bun run build:ci",
    "ci:changed": "bunx lint-staged && bun run type-check:ci",
    "verify": "lefthook run ci",
    "pre-push": "bun run ci:quick",
    "validate": "bun run type-check && bun run lint",
    "prebuild": "bun run type-check",
    "db:generate": "bunx prisma generate",
    "db:push": "bunx prisma db push",
    "db:migrate": "bunx prisma migrate dev",
    "db:migrate:deploy": "bunx prisma migrate deploy",
    "db:migrate:reset": "bunx prisma migrate reset",
    "db:seed": "bun run prisma/seed.ts",
    "db:studio": "bunx prisma studio",
    "postinstall": "bunx prisma generate"
  },
  "dependencies": {
    "@prisma/adapter-pg": "7.2.0",
    "@prisma/client": "^7.2.0",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
    "@tanstack/react-form": "^1.27.7",
    "@tanstack/react-query": "^5.90.16",
    "@tanstack/react-table": "^8.21.3",
    "@types/dompurify": "^3.2.0",
    "@vercel/analytics": "^1.6.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "dompurify": "^3.3.1",
    "isomorphic-dompurify": "2.35.0",
    "lru-cache": "11.2.4",
    "lucide-react": "^0.562.0",
    "motion": "12.23.26",
    "next": "16.1.1",
    "next-themes": "^0.4.6",
    "pg": "8.16.3",
    "postcss": "^8.5.6",
    "react": "^19.2.3",
    "react-day-picker": "^9.13.0",
    "react-dom": "^19.2.3",
    "react-error-boundary": "^6.0.1",
    "react-intersection-observer": "^10.0.0",
    "react-resizable-panels": "^4.2.0",
    "react-simple-typewriter": "^5.0.1",
    "recharts": "^3.6.0",
    "resend": "^6.6.0",
    "server-only": "0.0.1",
    "sharp": "^0.34.5",
    "sonner": "^2.0.7",
    "swiper": "^12.0.3",
    "vaul": "^1.1.2",
    "web-vitals": "^5.1.0",
    "zod": "^4.3.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@next/bundle-analyzer": "^16.1.1",
    "@next/eslint-plugin-next": "16.1.1",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/bun": "1.3.5",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.16",
    "babel-plugin-react-compiler": "^1.0.0",
    "concurrently": "^9.2.1",
    "critters": "0.0.25",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.1.1",
    "eslint-formatter-compact": "^9.0.1",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "fast-check": "4.5.3",
    "happy-dom": "20.0.11",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "lint-staged": "^16.2.7",
    "playwright": "^1.57.0",
    "prisma": "^7.2.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.51.0",
    "vitest": "^4.0.16"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "eslint --fix"
    ]
  }
}
